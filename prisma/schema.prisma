// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {dfd
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String        @id      @default(uuid())
  name         String        @unique
  email        String        @unique
  password     String   
  refreshToken String  

  @@map("user")
}